# Backend API Configuration
VITE_API_URL=http://localhost:3000/api

# Development settings
VITE_ENV=development

# Optional: API timeout in milliseconds
VITE_API_TIMEOUT=10000

# Stripe price IDs for Premium plans (frontend uses these to request checkout)
# Obtain these from your Stripe Dashboard and set in your real env (.env.local)
VITE_STRIPE_PRICE_THOUGHT_LEARNER=price_12345
VITE_STRIPE_PRICE_TRUTH_SEEKER=price_67890
VITE_STRIPE_PRICE_ULTRA_THINKER=price_ABCDE